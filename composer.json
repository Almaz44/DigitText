{
    "name": "andrey-helldar/digittext",
    "description": "The module allows to translate numbers into a text equivalent",
    "type": "library",
    "license": "MIT",
    "homepage": "http://ai-rus.com",
    "keywords": [
        "date",
        "time",
        "DateTime",
        "DigitText"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/DigitText/issues",
        "source": "https://github.com/andrey-helldar/DigitText"
    },
    "require": {
        "php": ">=7.0",
        "illuminate/http": "dev-master",
        "illuminate/pipeline": "dev-master",
        "illuminate/routing": "dev-master",
        "illuminate/session": "dev-master",
        "illuminate/support": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\DigitText\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Helldar\\DigitText\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Helldar\\DigitText\\ServiceProvider"
            ],
            "aliases": {
                "DigitText": "Helldar\\DigitText\\Facade"
            }
        }
    }
}
