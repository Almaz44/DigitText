{
    "name": "andrey-helldar/digittext",
    "description": "The module allows to translate numbers into a text equivalent",
    "type": "library",
    "license": "MIT",
    "homepage": "http://ai-rus.com",
    "keywords": [
        "date",
        "time",
        "DateTime",
        "DigitText"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/DigitText/issues",
        "source": "https://github.com/andrey-helldar/DigitText"
    },
    "require": {
        "php": ">=7.0",
        "illuminate/http": "5.5.*",
        "illuminate/routing": "5.5.*",
        "illuminate/session": "5.5.*",
        "illuminate/support": "5.5.*",
        "illuminate/pipeline": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\DigitText\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit; ./vendor/bin/php-cs-fixer fix -v --diff --dry-run;",
        "phpunit": "./vendor/bin/phpunit;",
        "phpcs": "./vendor/bin/php-cs-fixer fix -v --diff --dry-run;"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\DigitText\\ServiceProvider"
            ],
            "aliases": {
                "DigitText": "Helldar\\DigitText\\Facade"
            }
        }
    }
}
